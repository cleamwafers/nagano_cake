<h2>ジャンル編集</h2>

<%= form_with model: @genre, url: admin_genre_path do |f| %>
<div class="container">
  <table class="table">
    <tr>
      <td>
        ジャンル名
      </td>
      <td>
        <%= f.text_field :name %>
      </td>
      <td>
        <%= f.submit "変更を保存", class: "btn btn-success" %>
      </td>
    </tr>
  </table>
</div>
<% end %>
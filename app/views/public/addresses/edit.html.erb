<h2>配送先編集</h2>
<%= form_with(model:@address, url:public_addresses_path, local: true ,method: :get) do |f| %>
  <div class="field">
    <%= f.label :"郵便番号(ハイフンなし)" %>
    <%= f.text_field :postal_code, autocomplete: "postal_code" %>
  </div>

  <div class="field">
    <%= f.label :"住所" %>
    <%= f.text_field :address, autocomplete: "address",size:"60" %>
  </div>

   <div class="field">
    <%= f.label :"宛名" %>
    <%= f.text_field :name, autocomplete: "name" %>
    <%= f.submit "変更内容を保存", class: "btn btn-success" %>
  </div>

<% end %>

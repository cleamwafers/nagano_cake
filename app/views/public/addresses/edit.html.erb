<h2>配送先編集</h2>

  <div class="field">
    <%= f.label :"郵便番号(ハイフンなし)" %>
    <%= f.text_field :postal_code, autocomplete: "postal_code" %>
  </div>

  <div class="field">
    <%= f.label :"住所" %>
    <%= f.text_field :address, autocomplete: "address",size:"60" %>
  </div>

   <div class="field">
    <%= f.label :"宛名" %>
    <%= f.text_field :name, autocomplete: "name" %>
  </div>
  <%= f.submit "編集内容を保存", class: "btn btn-primary save col-xs-6" %>



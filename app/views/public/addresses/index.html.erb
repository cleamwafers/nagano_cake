 <h3><strong>配送先登録/一覧</strong></h3>
 <%= form_with model: @addresses , url: edit_public_address_path(address) do |f| %>

  <div class="field">
    <%= f.label :"郵便番号(ハイフンなし)" %>
    <%= f.text_field :postal_code, autocomplete: "postal_code" %>
  </div>

  <div class="field">
    <%= f.label :住所 %>
    <%= f.text_field :address, autocomplete: "address",size:"60" %>
  </div>

  <div class="field">
    <%= f.label :"宛名" %>
    <%= f.text_field :name, autocomplete: "name" %>
  </div>
<% end %>